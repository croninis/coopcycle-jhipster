                                 referencedTableName="livreur"/>

        <addForeignKeyConstraint baseColumnNames="commercant_id"
                                 baseTableName="commande"
                                 constraintName="fk_commande__commercant_id"
                                 referencedColumnNames="id"
                                 referencedTableName="commercant"/>
    </changeSet>
</databaseChangeLog>
